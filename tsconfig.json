{
    "compilerOptions": {
      "jsx": "react",
      "module": "commonjs",
      "noImplicitAny": true,
      "outDir": "./build/",
      "preserveConstEnums": true,
      "removeComments": true,
      "sourceMap": true,
      "esModuleInterop": true,
      "target": "es5"
    },
    "include": [
      "**/*.ts",
      "**/*.tsx"
    ],
  }